     "text": [
      "{'1': 1, '2': 4, '3': 9, '4': 16, '5': 25, '6': 36, '7': 49, '8': 64, '9': 81}\n"
     ]
    }
   ],
   "source": [
    "\n",
    "n = int(input(\"Enter a number: \"))\n",
    "\n",
    "# Create an empty dictionary\n",
    "result = {}\n",
    "\n",
    "# Generate dictionary items\n",
    "for x in range(1, n + 1):\n",
    "    result[str(x)] = x * x\n",
    "\n",
    "print(result)\n"
   ]
  }
